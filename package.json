{
  "name": "book-search",
  "private": true,
  "version": "0.1.0",
  "description": "An app for searching and viewing books",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "lint": "eslint",
    "lint:all": "eslint \"./src/**/*.{js,jsx}\"",
    "format": "prettier --write",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install"
  },
  "author": "Konstantin Samarin",
  "license": "MIT",
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "npm run lint"
    ],
    "*.{js,json,css,md}": [
      "npm run format"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "eslint": "^7.25.0",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "webpack": "^5.36.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loader-spinner": "^4.0.0",
    "react-promise-tracker": "^2.1.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samarink/book-search"
  },
  "bugs": {
    "url": "https://github.com/samarink/book-search/issues"
  }
}
